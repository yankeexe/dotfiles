[
  {
    "key": "alt+cmd+o",
    "command": "outline.focus"
  },
  {
    "key": "cmd+backspace",
    "command": "workbench.action.terminal.kill"
  },
  {
    "key": "cmd+u",
    "command": "workbench.action.terminal.unsplit"
  },
  {
    "key": "ctrl+cmd+m",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "cmd+l",
    "command": "-continue.focusContinueInput"
  },
  {
    "key": "shift+cmd+l",
    "command": "selectAllSearchEditorMatches",
    "when": "inSearchEditor"
  },
  {
    "key": "shift+cmd+l",
    "command": "-selectAllSearchEditorMatches",
    "when": "inSearchEditor"
  },
  {
    "key": "shift+cmd+l",
    "command": "-continue.focusContinueInputWithoutClear"
  },
  {
    "key": "shift+cmd+i",
    "command": "-workbench.action.quickchat.toggle",
    "when": "chatIsEnabled"
  },
  {
    "key": "cmd+; shift+cmd+i",
    "command": "-testing.toggleInlineCoverage"
  },
  {
    "key": "cmd+i",
    "command": "-cloudcode.duetAI.showinEditor",
    "when": "authLoggedIn && config.cloudcode.duetAI.enable && editorTextFocus && shouldRegisterInEditorCommand && !inGeminiDiffView && !isInDiffEditor"
  },
  {
    "key": "shift+cmd+enter",
    "command": "python.execInTerminal-icon"
  },
  {
    "key": "shift+cmd+i",
    "command": "-workbench.panel.chatEditing",
    "when": "workbench.panel.chat.view.edits.active"
  },
  {
    "key": "shift+cmd+i",
    "command": "-continue.focusEditWithoutClear"
  },
  {
    "key": "shift+cmd+i",
    "command": "-workbench.action.chat.openEditSession",
    "when": "chatEditingParticipantRegistered && chatIsEnabled && view != 'workbench.panel.chat.view.edits'"
  },
  {
    "key": "shift+cmd+i",
    "command": "-workbench.action.chat.openEditSession",
    "when": "chatEditingParticipantRegistered && view != 'workbench.panel.chat.view.edits'"
  },
  {
    "key": "shift+cmd+i",
    "command": "cloudcode.duetAI.actionsMenu"
  },
  {
    "key": "cmd+y",
    "command": "workbench.action.switchWindow"
  },
  {
    "key": "ctrl+w",
    "command": "-workbench.action.switchWindow"
  },
  {
    "key": "alt+l",
    "command": "workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+cmd+right",
    "command": "-workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+h",
    "command": "workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+cmd+left",
    "command": "-workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "shift+alt+c",
    "command": "git.checkout"
  },
]

